<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>FileUploadServlet</servlet-name>
    <servlet-class>com.sundy.fileupload.servlet.FileUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUploadServlet</servlet-name>
    <url-pattern>/FileUploadServlet.htm1</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FileMergeServlet</servlet-name>
    <servlet-class>com.sundy.fileupload.servlet.FileMergeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileMergeServlet</servlet-name>
    <url-pattern>/FileMergeServlet.htm1</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FileCheckServlet</servlet-name>
    <servlet-class>com.sundy.fileupload.servlet.FileCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileCheckServlet</servlet-name>
    <url-pattern>/FileCheckServlet.htm1</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CacheFileCheckServlet</servlet-name>
    <servlet-class>com.sundy.fileupload.servlet.CacheFileCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CacheFileCheckServlet</servlet-name>
    <url-pattern>/CacheFileCheckServlet.htm1</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>fileuploader.jsp</welcome-file>
  </welcome-file-list>
</web-app>